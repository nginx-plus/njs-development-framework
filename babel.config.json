{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "debug": true,
        "corejs": "3.21",
        "include": ["es.set", "es.symbol"],
        "exclude": [
          "es.json.stringify", // the polyfill for stringify is incompatible with njs' regex engine, pcre
          "@babel/plugin-transform-block-scoping", // njs supports `let` and `const`
          "es.array.map", // njs supports
          "es.regexp.exec",
          "es.string.split",
          "es.function.name"
        ]
      }
    ]
  ]
}

